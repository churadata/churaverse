{
  "name": "phaser-spine-game-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run versionUpdate vite",
    "build": "npm run versionUpdate tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "format": "prettier --write src",
    "versionUpdate": "VITE_DEPLOY_VERSION=$(date +%s):$(cat deploy_version.txt) VITE_FRONT_VERSION=$npm_package_version"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-filename-rules": "^1.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "tiled-types": "^1.3.0",
    "typescript": "^4.6.3",
    "vite": "^4.0.1"
  },
  "dependencies": {
    "livekit-client": "^1.9.7",
    "phaser": "^3.55.2",
    "phaser3-nineslice": "^0.5.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
